{
  "name": "redux-universal-example",
  "private": true,
  "version": "1.0.0",
  "description": "Starter kit with universally-rendered Redux application",
  "scripts": {
    "start": "node server/index.js",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "clean": "npm run remove-dist && mkdir dist",
    "build:client": "NODE_ENV=production webpack --config ./config/webpack.client.prod.js --progress --hide-modules && cp -R ./static ./dist",
    "build:server": "NODE_ENV=production webpack --config ./config/webpack.config.server.js --progress --hide-modules",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "gh-build:client": "NODE_ENV=gh webpack --config ./config/webpack.client.prod.js --progress --hide-modules && cp -R ./static ./dist",
    "gh-build:server": "NODE_ENV=gh webpack --config ./config/webpack.config.server.js --progress --hide-modules",
    "gh-build": "npm run clean && npm run gh-build:client && npm run gh-build:server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactjs/redux.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactjs/redux/issues"
  },
  "homepage": "http://redux.js.org",
  "dependencies": {
    "alertify-js": "0.3.12",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-react-optimize": "1.0.1",
    "babel-register": "^6.4.3",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "css-loader": "0.26.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "1.0.1",
    "qs": "^4.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-helmet": "3.2.3",
    "react-redux": "^4.2.1",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.7",
    "redux": "^3.2.1",
    "redux-thunk": "^1.0.3",
    "reselect": "2.5.4",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "serialize-javascript": "1.3.0",
    "serve-static": "1.10.0",
    "style-loader": "0.13.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-runtime": "^6.3.13",
    "eslint": "3.12.1",
    "eslint-config-airbnb-base": "11.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-html": "1.7.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.8.0",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "2.24.1",
    "purifycss-webpack-plugin": "2.0.3",
    "url-loader": "0.5.7",
    "webpack": "^1.11.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1",
    "webpack-merge": "1.1.1"
  }
}
